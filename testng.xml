<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <test verbose="2" preserve-order="true" name="LoginTest_Chrome">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="url" value="https://www.veridianhealthcare.com/login/"></parameter>
        <classes>
            <class name="tests.LoginTest"></class>
        </classes>
    </test>


    <test verbose="2" preserve-order="true" name="LoginTest_fireFox">
        <parameter name="browserName" value="firefox"></parameter>
        <parameter name="url" value="https://www.veridianhealthcare.com/login/"></parameter>
        <classes>
            <class name="tests.LoginTest"></class>
        </classes>
    </test>
</suite>